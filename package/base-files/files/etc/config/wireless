
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/18000000.wmac'
	option channel '1'
	option band '2g'
	option htmode 'HE20'
	option disabled '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/18000000.wmac+1'
	option channel '36'
	option band '5g'
	option htmode 'HE80'
	option country 'CN'
	option cell_density '3'
	option disabled '0'

config wifi-device 'radio2'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi'
	option channel '1'
	option band '2g'
	option htmode 'HE20'
	option disabled '0'

config wifi-device 'radio3'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi+1'
	option channel '36'
	option band '5g'
	option htmode 'HE160'
	option cell_density '3'
	option country 'US'
	option disabled '1'

config wifi-iface 'wifinet0'
	option device 'radio3'
	option mode 'ap'
	option ssid 'Chasm'
	option encryption 'psk2'
	option network 'lan'
	option key 'd27k56l98n61'

